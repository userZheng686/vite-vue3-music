{
  "name": "neteasemusic",
  "version": "0.0.0",
  "main": "dist/electron-main/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "rimraf dist && vite build",
    "package": "electron-builder",
    "serve": "vite preview",
    "git": "git add . && git cz && git push"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.5",
    "@types/js-cookie": "^3.0.2",
    "@vueuse/components": "^8.7.5",
    "@vueuse/core": "^8.7.5",
    "acrcloud": "^1.4.0",
    "axios": "^0.27.2",
    "blueimp-md5": "^2.19.0",
    "crypto-js": "^4.1.1",
    "default-passive-events": "^2.0.0",
    "electron-store": "^8.0.2",
    "element-china-area-data": "^5.0.2",
    "element-plus": "^2.2.6",
    "ffmetadata": "^1.7.0",
    "install": "^0.13.0",
    "js-base64": "^3.7.2",
    "md5": "^2.3.0",
    "mitt": "^3.0.0",
    "music-metadata": "^7.12.4",
    "npm": "^8.12.2",
    "pinia": "^2.0.14",
    "qrcode.vue": "^3.3.3",
    "qrcodejs2": "^0.0.2",
    "vite-plugin-optimizer": "^1.4.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue3-menus": "^1.1.2",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@iconify/json": "^2.1.63",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/jsmediatags": "^3.9.3",
    "@types/node": "^18.0.6",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.37",
    "@vuedx/typescript-plugin-vue": "^0.7.5",
    "@vueuse/electron": "^8.9.2",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.5",
    "cz-customizable": "^7.0.0",
    "electron": "^19.0.8",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint-plugin-vue": "^9.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.8.8",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^3.0.2",
    "vite-plugin-commonjs": "^0.5.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-electron": "^0.4.5",
    "vite-plugin-inspect": "^0.7.5",
    "vite-plugin-require-transform": "^1.0.3",
    "vue-tsc": "^0.3.0"
  },
  "build": {
    "appId": "com.neteaseMusic.desktop",
    "productName": "neteaseMusic",
    "electronVersion" : "1.0.0",
    "asar": true,
    "copyright": "Copyright © 2022 neteaseMusic",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "./public/icon.ico",
      "requestedExecutionLevel": "requireAdministrator",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "guid" : "neteaseMusic",
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "installerIcon": "./public/icon.ico",
      "uninstallerIcon": "./public/icon.ico",
      "installerHeaderIcon": "./public/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "网易云音乐"
    },
    
    "publish": [
      {
        "provider": "generic",
        "url": "https://neteasemusic.netlify.app/#/upgrade"
      }
    ],
    "releaseInfo": {
      "releaseNotes": "版本更新的具体内容"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
